# Banger POS Mobile Build Environment Variables
# Copy this file to .env and fill in the values

# Required: Production API URL (your Vercel deployment)
NEXT_PUBLIC_API_URL=https://pos.bangerpos.com

# Required: Supabase credentials (same as web)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Set automatically during mobile build
NEXT_PUBLIC_CAPACITOR=true

# Capgo OTA Updates (optional, for live updates)
CAPGO_API_KEY=your-capgo-api-key
